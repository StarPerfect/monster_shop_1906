<% @merchants.each do |merchant| %>

<section id="merch-<%= merchant.id %>">
  <%= link_to merchant.name, admin_merchant_show_path(merchant) %>
  <%= merchant.city %>
  <%= merchant.state %>
  <% if merchant.enabled? == true %>
    <%= link_to 'Disable', able_path(merchant), method: :patch %>
  <% else %>
    <%= link_to 'Enable', able_path(merchant), method: :patch %>
  <% end %>
</section>

<% end %>
