<center>
  <h1>Admin Dashboard</h1>
  <p><%= link_to 'Merchant Status', '/admin/merchants' %></p>
<% @orders.each do |order| %>
  <section id="order-<%= order.id %>">
  <%= order.id %>
  <%= order.created_at %>
  <%= link_to order.name, admin_user_show_path(order.user_id) %>
  <%= order.status %>
  <% if order.packaged? %>
    <%= link_to "Ship Order", admin_ship_order_path(order), method: :patch %>
  <% end %>
</section>
<% end %>
</center>
